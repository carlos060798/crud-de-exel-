{
  "name": "crud-de-exel",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "src/server",
    "src/client"
  ],
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "tsc && npm run build:client && cp -r src/client/build dist/client",
    "build:client": "cd src/client && npm run build",
    "dev": "concurrently \"npm run watch:server\" \"npm run start:client\"",
    "watch:server": "tsc -w",
    "server": "nodemon dist/server/index.js",
    "start:client": "cd src/client && npm start",
    "dev:full": "concurrently \"npm run server\" \"npm run start:client\""
  },
  "dependencies": {
    "@types/node": "^16.18.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "mssql": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.0",
    "typescript": "^4.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "tailwindcss": "^3.3.0",
    "ts-node-dev": "^2.0.0"
  }
}
